<template name="navmenu">
  <block wx:for="{{array}}">
      <view class="item" bindtap="itemClick" id="{{index}}">
        <view class="img">
          <image src="{{item.img}}" class="item_img"></image>
        </view>
        <view class="item_title">{{item.title}}</view>
      </view>
    </block>
</template>